<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--<bean id="user" class="com.atguigu.spring5.bean.User"></bean>-->

    <bean id="book" class="com.atguigu.spring5.bean.Booke">
        <property name="bookName" value="九阳真经"></property>
        <property name="author" value="王思聪"></property>
    </bean>

    <bean id="car" class="com.atguigu.spring5.bean.Car">
        <constructor-arg name="carName" value="凯迪拉克"></constructor-arg>
        <constructor-arg name="price" value="20"></constructor-arg>
    </bean>
</beans>